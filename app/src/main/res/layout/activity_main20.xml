<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bb"
    tools:context=".MainActivity20">


    <Button
        android:id="@+id/textView"
        android:layout_width="298dp"
        android:layout_height="88dp"
        android:layout_marginTop="40dp"
        android:background="@color/colorPrimaryDark"
        android:fontFamily="@font/benchnine_bold"
        android:gravity="center_horizontal|center_vertical"
        android:onClick="openLec"
        android:text="VIDEOS"
        android:textColor="@color/colorWhite"
        android:textSize="48sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <Button
        android:id="@+id/textView2"
        android:layout_width="298dp"
        android:layout_height="138dp"
        android:layout_marginTop="40dp"
        android:background="@color/colorPrimaryDark"
        android:fontFamily="@font/benchnine_bold"
        android:gravity="center_horizontal|center_vertical"
        android:onClick="openPYQ"
        android:text="Previous Year Questions"
        android:textColor="@color/colorWhite"
        android:textSize="48sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/textView3"
        android:layout_width="298dp"
        android:layout_height="88dp"
        android:layout_marginTop="40dp"
        android:background="@color/colorPrimaryDark"
        android:fontFamily="@font/benchnine_bold"
        android:gravity="center_horizontal|center_vertical"
        android:onClick="openFormula"
        android:text="FORMULA SHEET"
        android:textColor="@color/colorWhite"
        android:textSize="48sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Button
        android:id="@+id/textView4"
        android:layout_width="298dp"
        android:layout_height="88dp"
        android:layout_marginTop="107dp"
        android:background="@color/colorPrimaryDark"
        android:fontFamily="@font/benchnine_bold"
        android:gravity="center_horizontal|center_vertical"
        android:onClick="openBook"
        android:text="BOOKS"
        android:textColor="@color/colorWhite"
        android:textSize="48sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="364dp"
        android:layout_height="34dp"
        android:gravity="center_horizontal|center_vertical"
        android:onClick="openQuiz"
        android:text="( Check Out If there are quizes )"
        android:textColor="@color/colorWhite"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.964" />

</androidx.constraintlayout.widget.ConstraintLayout>